  <!-- 機能のロックダウン（全てfalseでSTS専用モードで動作） -->
  
  <!-- サインアップ -->
  <add key="EnableSignupProcess" value="true" />
  <!-- ユーザ属性編集 -->
  <add key="EnableEditingOfUserAttribute" value="true" />
  <!-- ユーザ・ロール管理 -->
  <add key="EnableAdministrationOfUsersAndRoles" value="true" />

  <!-- ID連携 -->
